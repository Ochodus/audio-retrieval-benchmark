{
    "inherit_from": "configs/base_config.json",
    "eval_mode": "test_run",
    "experts": {
        "text_feat": "bert",
        "modalities": [
            "imagenet.resnext101_32x48d.0",
            "r2p1d.r2p1d-ig65m.0",
            "scene.densenet161.0",
            "audio",
            "ast"
        ]
    },
    "arch": {
        "type": "CENet",
        "args": {
            "test_caption_mode": "indep",
            "use_ce": "pairwise",
            "use_mish": 1,
            "use_bn_reason": 1,
            "num_g_layers": 3,
            "num_h_layers": 0,
            "include_self": 1,
            "l2renorm": false,
            "randomise_feats": "",
            "vlad_clusters": {
                "text": 20,
                "audio": 16,
                "pann": 16,
                "syncnet": 16,
                "vggsound": 16,
                "speech": 5,
                "ast": 16
            },
            "ghost_clusters": {
                "text": 1
            },
            "mimic_ce_dims": 0
        }
    },
    "optimizer": {
        "type": "Ranger",
        "args": {
            "lr": 0.01,
            "weight_decay": 1E-03
        }
    },
    "loss": {
        "type": "MaxMarginRankingLoss",
        "args": {
            "margin": 0.2,
            "fix_norm": true
        }
    },
    "data_loader": {
        "type": "ExpertDataLoader",
        "args":{
            "dataset_name": "AudioCaps",
            "data_dir": "data/AudioCaps",
            "root_feat_folder": "structured-symlinks",
            "trn_cat": 0,
            "batch_size": 128,
            "split_name": "val",
            "fuse_captions": false,
            "num_test_captions": 1,
            "max_tokens": {
                "text": 20,
                "audio": 29,
                "pann": 29,
                "syncnet": 29,
                "vggsound": 29,
                "speech": 35,
                "ast": 29
            },
            "feat_aggregation": {
                "imagenet.senet154.0": {
                    "fps": 25,
                    "stride": 1,
                    "pixel_dim": 256,
                    "aggregate-axis": 1,
                    "offset": 0,
                    "temporal": "avg",
                    "aggregate": "concat",
                    "type": "embed",
                    "feat_dims": {
                        "embed": 2048,
                        "logits": 1000
                    }
                },
                "imagenet.resnext101_32x48d.0": {
                    "fps": 25,
                    "stride": 1,
                    "offset": 0,
                    "pixel_dim": 256,
                    "temporal": "avg",
                    "aggregate": "concat",
                    "aggregate-axis": 1,
                    "type": "embed",
                    "feat_dims": {
                        "embed": 2048,
                        "logits": 1000
                    }
                },
                "scene.densenet161.0": {
                    "stride": 1,
                    "fps": 25,
                    "offset": 0,
                    "temporal": "avg",
                    "pixel_dim": 256,
                    "aggregate": "concat",
                    "aggregate-axis": 1,
                    "type": "embed",
                    "feat_dims": {
                        "embed": 2208,
                        "logits": 1000
                    }
                },
                "i3d.i3d.0": {
                    "fps": 25,
                    "offset": 0,
                    "stride": 25,
                    "inner_stride": 1,
                    "pixel_dim": 256,
                    "temporal": "avg",
                    "aggregate": "concat",
                    "aggregate-axis": 1,
                    "type": "embed",
                    "feat_dims": {
                        "embed": 1024,
                        "logits": 400
                    }
                },
                "r2p1d.r2p1d-ig65m.0": {
                    "fps": 30,
                    "offset": 0,
                    "stride": 32,
                    "inner_stride": 1,
                    "pixel_dim": 256,
                    "temporal": "avg",
                    "aggregate": "concat",
                    "aggregate-axis": 1,
                    "type": "embed",
                    "feat_dims": {
                        "embed": 512,
                        "logits": 359
                    }
                },
                "r2p1d.r2p1d-ig65m-kinetics.0": {
                    "fps": 30,
                    "offset": 0,
                    "stride": 32,
                    "inner_stride": 1,
                    "pixel_dim": 256,
                    "temporal": "avg",
                    "aggregate": "concat",
                    "aggregate-axis": 1,
                    "type": "embed",
                    "feat_dims": {
                        "embed": 512,
                        "logits": 400
                    }
                },
                "pann.pann.0": {
                    "model": "pann",
                    "flaky": false,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "pann": {
                    "model": "pann",
                    "flaky": false,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "syncnet": {
                    "model": "syncnet",
                    "flaky": false,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "audio.syncnet.0": {
                    "model": "syncnet",
                    "flaky": false,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "vggsound": {
                    "model": "vggsound",
                    "flaky": false,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "audio.vggsound.0": {
                    "model": "vggsound",
                    "flaky": false,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "speech": {
                    "model": "w2v",
                    "flaky": true,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false,
                    "feat_dims": {
                        "embed": 300
                    }
                },
                "audio": {
                    "model": "vggish",
                    "flaky": true,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "audio.vggish.0": {
                    "model": "vggish",
                    "flaky": true,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                },
                "ast": {
                    "model": "ast",
                    "flaky": true,
                    "temporal": "vlad",
                    "type": "embed",
                    "binarise": false
                }
            }
        }
    },
    "metrics": [
        "t2v_metrics",
        "v2t_metrics"
    ],
    "trainer": {
        "epochs": 20
    },
    "eval_settings": {
        "data_loader": {
            "args": {
                "split_name": "test",
                "num_test_captions": 5
            }
        },
        "tester": {
            "save_dir": "data/saved/",
            "verbosity": 2
        },
        "disable_gpu": true
    },
    "testing_file": "final_filtered_test_list.txt",
    "visualizer": {
        "type": "Visualizer",
        "args":{
            "src_video_dir": "data/AudioCaps/videos",
            "vis_vid_freq": 500,
            "num_samples": 5
        }
    }
}
